#Work in progress - pulls and parses HTML from one review on CNet
#Completed script will pull from multiple reviews on CNet and create flat file to load into GCP bucket

import requests
from bs4 import BeautifulSoup

review = 'https://www.cnet.com/reviews/dell-xps-13-2020-review/'
review_page = requests.get(review)
review_soup = BeautifulSoup(review_page.content, 'html.parser')

results = review_soup.find(id='page-dell-xps-12-2020')
rating = results.find_all('div', class_='c-metaCard_rating')
body = results.find_all('p')
title = results.find_all('h1')
author = results.find_all('a', class_='author')
date = results.find_all('time')

